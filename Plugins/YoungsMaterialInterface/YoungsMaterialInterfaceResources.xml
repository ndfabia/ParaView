<ServerManagerConfiguration>
  <ProxyGroup name="filters">

    <SourceProxy name="DataSetGradientPrecompute" class="vtkDataSetGradientPrecompute" label="Gradient Precomputation">
	<InputProperty
		    name="Input"
		    command="SetInputConnection">
	      <ProxyGroupDomain name="groups">
		      <Group name="sources"/>
		      <Group name="filters"/>
	      </ProxyGroupDomain>
	      <DataTypeDomain name="input_type">
		      <DataType value="vtkDataSet"/>
	      </DataTypeDomain>
      </InputProperty>
           <Documentation
            short_help="This filter precomputes a geometry based vector field, that has to be applied prior to usage of vtkDataSetGradient."
	    long_help="No scalars has to be specified, this filter precomputes values based on the mesh only, the vtkDataSetGradient then produces the gradient of a specific scalar field using these precomputed values ofr better performances">
          </Documentation>
   </SourceProxy>
	
   <SourceProxy name="DataSetGradient" class="vtkDataSetGradient" label="DataSet Gradient">
      <Documentation
	long_help="Computes the gradient of a specified cell or point scalar field. It works on arbitrary mesh topology composed of any cell types"
	short_help="Scalar gradient computation for unstructured meshes">
      </Documentation>
 
	    <InputProperty
       name="Input"
       command="SetInputConnection">
          <ProxyGroupDomain name="groups">
            <Group name="sources"/>
            <Group name="filters"/>
          </ProxyGroupDomain>
          <DataTypeDomain name="input_type">
            <DataType value="vtkDataSet"/>
          </DataTypeDomain>
     </InputProperty>

     <StringVectorProperty 
        name="SelectInputScalars" 
        command="SetInputArrayToProcess"
        number_of_elements="5"
        element_types="0 0 0 0 2"
        animateable="0"
        label="Input Scalar Field">
          <ArrayListDomain name="array_list" attribute_type="Scalars">
            <RequiredProperties>
               <Property name="Input" function="Input"/>
            </RequiredProperties>
          </ArrayListDomain>
          <FieldDataDomain name="field_list">
            <RequiredProperties>
              <Property name="Input" function="Input" />
            </RequiredProperties>
          </FieldDataDomain>
          <Documentation>
            This property specifies the name of the scalar array to compute the gradient of.
          </Documentation>
     </StringVectorProperty>

     <StringVectorProperty
	name="Result array name"
	command="SetResultArrayName"
	number_of_elements="1"
	default_values="gradient" >
          <Documentation>
            This property specifies the name of the resulting gradient array.
          </Documentation>
      </StringVectorProperty>
      
    </SourceProxy>

    <SourceProxy name="YoungsMaterialInterface" class="vtkYoungsMaterialInterface" label="Youngs material interface reconstruction">
      <Documentation
		    long_help="Computes linear material interfaces in 2D or 3D mixed cells produced by eulerian or ALE simulation codes"
		    short_help="Youngs material interface reconstruction">
      </Documentation>
      
      <InputProperty
		    name="Input"
		    command="SetInputConnection">
	      <ProxyGroupDomain name="groups">
		      <Group name="sources"/>
		      <Group name="filters"/>
	      </ProxyGroupDomain>
	      <DataTypeDomain name="input_type">
		      <DataType value="vtkDataSet"/>
	      </DataTypeDomain>
        <InputArrayDomain name="input_array1" attribute_type="cell"
                          number_of_components="1" optional="0"/>
        <InputArrayDomain name="input_array3" attribute_type="cell"
                          number_of_components="3" optional="0"/>
      </InputProperty>

      <IntVectorProperty
			name="InverseNormal"
			command="SetInverseNormal"
			number_of_elements="1"
			default_values="0" >
	      <BooleanDomain name="bool"/>
          <Documentation>
            This property specifies if the interface normal vector should be inversed prior to interrface reconstruction
          </Documentation>
      </IntVectorProperty>

      <IntVectorProperty
			name="ReverseMaterialOrder"
			command="SetReverseMaterialOrder"
			number_of_elements="1"
			default_values="0" >
	      <BooleanDomain name="bool"/>
      </IntVectorProperty>

      <IntVectorProperty
			name="OnionPeel"
			command="SetOnionPeel"
			number_of_elements="1"
			default_values="1" >
	      <BooleanDomain name="bool"/>
      </IntVectorProperty>

      <IntVectorProperty
			name="AxisSymetric"
			command="SetAxisSymetric"
			number_of_elements="1"
			default_values="1" >
	      <BooleanDomain name="bool"/>
      </IntVectorProperty>

      <IntVectorProperty
			name="FillMaterial"
			command="SetFillMaterial"
			number_of_elements="1"
			default_values="1" >
	      <BooleanDomain name="bool"/>
      </IntVectorProperty>

      <IntVectorProperty
			name="UseFractionAsDistance"
			command="SetUseFractionAsDistance"
			number_of_elements="1"
			default_values="0" >
	      <BooleanDomain name="bool"/>
      </IntVectorProperty>

     <IntVectorProperty
			name="TwoMaterialsOptimization"
			command="SetTwoMaterialsOptimization"
			number_of_elements="1"
			default_values="0" >
	      <BooleanDomain name="bool"/>
      </IntVectorProperty>


	<DoubleVectorProperty
			   name="VolumeFractionRange"
			   command="SetVolumeFractionRange"
			   number_of_elements="2"
			   default_values="0.01 0.99" >
	      <DoubleRangeDomain name="range" />
      </DoubleVectorProperty>

      <!--
     <StringVectorProperty 
        name="Material1" 
	label="Material1"
        command="SetMaterialArrays"
        repeat_command="1"
        number_of_elements_per_command="4" 
        number_of_elements="4" 
	element_types="0 2 2 2" >
        <Documentation>
          This property adds a new material.
        </Documentation>
     </StringVectorProperty>

     <StringVectorProperty 
        name="Material2" 
	label="Material2"
        command="SetMaterialArrays"
        repeat_command="1"
        number_of_elements_per_command="4" 
        number_of_elements="4" 
	element_types="0 2 2 2" >
        <Documentation>
          This property adds a new material.
        </Documentation>
     </StringVectorProperty>
     -->

	<IntVectorProperty
			name="NumberOfMaterials"
			command="SetNumberOfMaterials"
			number_of_elements="1"
			default_values="0" >
    </IntVectorProperty>

     <StringVectorProperty 
        name="Material" 
	      label="Add Material"
        command="SetMaterialArrays"
        clean_command="RemoveAllMaterials"
        number_of_elements_per_command="4" 
        repeat_command="1"
	      element_types="0 2 2 2" >
        <Documentation>
          This property adds a new material.
        </Documentation>
        <ArrayListDomain 
                name="scalar_list" attribute_type="Scalars"
                input_domain_name="input_array1">
          <RequiredProperties>
             <Property name="Input" function="Input"/>
          </RequiredProperties>
        </ArrayListDomain>
        <ArrayListDomain 
                name="vector_list" attribute_type="Vectors"
                input_domain_name="input_array3">
          <RequiredProperties>
             <Property name="Input" function="Input"/>
          </RequiredProperties>
        </ArrayListDomain>
     </StringVectorProperty>

    </SourceProxy>

  </ProxyGroup>

</ServerManagerConfiguration>
